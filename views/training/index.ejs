<!DOCTYPE html>
<html lang="en">
<head>

<!-- Google Analytics -->
<% include ../partials/ga-includes %>

<!-- META SETTINGS -->
<% include ../partials/meta-settings.ejs %>

<!-- PAGE TITLE -->
<title><%- training.title %></title>

<% if (training.slug === "register") { %>
    <meta name="robots" content="noindex">
<% } %>

<% if (training.slug !== "register") { %>
<!-- OG TAGS -->
<meta property="og:url" content="https://www.pointwise.com/training/<%- training.slug %>"/>
<meta property="og:type" content="website"/>
<meta property="og:title" content="<%- training.title %>"/>
<meta property="og:description" content="<%- training.description %>"/>
<% } %>

<!-- META DATA -->
<meta name="description" content="<%- training.description %>">
<meta name="keywords" content="cfd meshing, mesh generation, grid generation, computational fluid dynamics, mesh generator, cfd software, cfd consultants, mesh software, grid software, fluid dynamics modeling"/>

<!-- CSS FILES -->
<% include ../partials/css-includes %>

</head>

<body id="page-top">

<!-- TOP NAVIGATION -->
<% include ../partials/navbar %>

<!-- PAGE COPY -->
<% if (training.slug === "index.html") { %>
    <%- training.copy %>

    <div class="col-lg-1">
    </div>
    <div class="col-lg-3">
        <div class="support-nav cta-space">
            <% include ../partials/nav-support %>
        </div>
    </div>
    </div>

    </div>
    </div>
<% } else { %>
    <div class="container support-body">
        <div class="row">

            <div class="col-lg-8">
                <% include ../partials/form-training %>
            </div>
            <div class="col-lg-1">
            </div>
            <div class="col-lg-3">
                <div class="support-nav cta-space">
                    <% include ../partials/nav-support %>
                </div>
            </div>
        </div>
    </div>
<% } %>

</div>
</div>

<!-- FOOTER -->
<% include ../partials/footer %>

<!-- JAVASCRIPT -->
<% include ../partials/js-includes %>
<script src="../js/tab-fragment.js" defer></script>

</body>
</html>
