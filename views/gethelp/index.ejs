<!DOCTYPE html>
<html lang="en">
<head>

<!-- Google Analytics -->
<% include ../partials/ga-includes %>

<!-- META SETTINGS -->
<% include ../partials/meta-settings.ejs %>

<!-- PAGE TITLE -->
<title><%- getHelp.title %></title>

<% if (getHelp.slug === "form") { %>
    <meta name="robots" content="noindex">
<% } %>

<% if (getHelp.slug !== "form") { %>
<!-- OG TAGS -->
<meta property="og:url" content="https://www.pointwise.com/gethelp/<%- getHelp.slug %>" />
<meta property="og:type" content="website" />
<meta property="og:title" content="<%- getHelp.title %>" />
<meta property="og:description" content="<%- getHelp.description %>" />

<!-- META DATA -->
<meta name="description" content="<%- getHelp.description %>">
<meta name="keywords" content="cfd meshing, mesh generation, grid generation, computational fluid dynamics, mesh generator, cfd software, cfd consultants, mesh software, grid software, fluid dynamics modeling" />
<% } %>

<!-- CSS FILES -->
<% include ../partials/css-includes %>

<!-- RECAPTCHA -->
<script src="https://www.google.com/recaptcha/api.js"></script>

</head>

<body id="page-top">

  <!-- TOP NAVIGATION -->
  <% include ../partials/navbar %>


  <!-- PAGE COPY -->
  <% if (getHelp.slug === "index.html") { %>
  <%- getHelp.copy %>

  <div class="col-lg-1">
  </div>
      <div class="col-lg-3">
          <% include ../partials/form-support %>

          <br />

          <div class="support-nav cta-space">
              <% include ../partials/nav-support %>
          </div>
    </div>
  </div>

</div>
</div>
  <% } else { %>
  <div class="container support-body">
      <div class="row">

          <div class="col-lg-8">
              <% include ../partials/form-gethelp %>
          </div>
      </div>
  </div>
  <% } %>

<!-- FOOTER -->
<% include ../partials/footer %>

<!-- JAVASCRIPT -->
<% include ../partials/js-includes %>
<script src="../js/gethelpAnchorOpen.js" defer></script>

</body>
</html>
