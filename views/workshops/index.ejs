<!DOCTYPE html>
<html lang="en">
<head>

    <!-- Google Analytics -->
    <% include ../partials/ga-includes %>

    <!-- META SETTINGS -->
    <% include ../partials/meta-settings.ejs %>

	<!-- PAGE TITLE -->
	<title><%- workshops.title %></title>

	<% if (workshops.gated === true) { %>
		<meta name="robots" content="noindex">
	<% } %>

	<% if (workshops.gated === false) { %>
	<!-- OG TAGS -->
	<meta property="og:url" content="https://www.pointwise.com/workshops/<%- workshops.slug %>" />
	<meta property="og:type" content="website" />
	<meta property="og:title" content="<%- workshops.title %>" />
	<meta property="og:description" content="<%- workshops.description %>" />

	<!-- META DATA -->
	<meta name="description" content="<%- workshops.description %>">
	<meta name="keywords" content="cfd meshing, mesh generation, grid generation, computational fluid dynamics, mesh generator, cfd software, cfd consultants, mesh software, grid software, fluid dynamics modeling" />
	<% } %>

	<!-- CSS FILES -->
	<% include ../partials/css-includes %>

</head>

<body id="page-top">

	<!-- TOP NAVIGATION -->
	<% include ../partials/navbar %>

    <!-- PAGE COPY -->
    <%- workshops.copy %>

    <!-- FOOTER -->
    <% include ../partials/footer %>

	<!-- JAVASCRIPT -->
	<% include ../partials/js-includes %>

</body>
</html>
