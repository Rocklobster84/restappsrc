<!DOCTYPE html>
<html lang="en">
<head>

    <!-- Google Analytics -->
    <% include ../partials/ga-includes %>

    <!-- META SETTINGS -->
    <% include ../partials/meta-settings.ejs %>

	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<!-- PAGE TITLE -->
	<title><%- support.title %></title>

	<!-- OG TAGS -->
	<meta property="og:url" content="https://www.pointwise.com/support/<%- support.slug %>" />
	<meta property="og:type" content="website" />
	<meta property="og:title" content="<%- support.title %>" />
	<meta property="og:description" content="<%- support.description %>" />

	<!-- META DATA -->
	<meta name="description" content="<%- support.description %>">
	<meta name="keywords" content="cfd meshing, mesh generation, grid generation, computational fluid dynamics, mesh generator, cfd software, cfd consultants, mesh software, grid software, fluid dynamics modeling" />

	<!-- CSS FILES -->
	<% include ../partials/css-includes %>

</head>

<body id="page-top">

	<!-- TOP NAVIGATION -->
	<% include ../partials/navbar %>

    <!-- PAGE COPY -->
    <% if (support.slug === "index.html") { %>
        <!-- Landing Page -->
        <%- support.copy %>
    <% } else { %>
        <!-- Release Notes Pages -->
        <div class="container">
        <div class="row">
        <div class="col-lg-8">
            <%- support. copy %>
            <section id="previous">
                <% include ../partials/support-versions %>
			</section>
		</div>
		<!-- Space between columns -->
		<div class="col-lg-1">
		</div>

		<!-- Column Contents -->
		<div class="col-lg-3">
			<div class="page-nav">
				<% include ../partials/nav-support %>
			</div>
		</div>
		</div>
		</div>
    <% } %>

	<!-- FOOTER -->
	<% include ../partials/footer %>

	<!-- JAVASCRIPT -->
	<% include ../partials/js-includes %>
</body>
</html>
